package abstractTest;

import java.util.Iterator;

public class Test {
	public static void main(String[] args) {
	}
}

class Solution {
    public int[] solution(int[] grade) {
        int[] answer = new int[grade.length];
        int temp = 0;
        int index = 0;
        for (int i = 0; i < grade.length; i++) {
			if(grade[i] > temp) {
				temp = grade[i];
			}
		}
        
        
        
        return answer;
    }
}